<%- include('layout') -%>
<br><br><br>
<div class="container">
<table class="table table-striped table-hover table-responsive">
  <tr>
    <th>ID</th>
    <th>Nome</th>
    <th>Idade</th>
  </tr>
  <% alunos.forEach(aluno => { %>
  <tr>
    <td><%= aluno.id %></td>
    <td><%= aluno.nome %></td>
    <td><%= aluno.idade %></td>
  </tr>
  <% }); %>
</table>

</div>

